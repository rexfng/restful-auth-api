{
  "name": "RESTful + AUTH API",
  "description": "RESTful Backend in Node.js via Express",
  "repository": "https://github.com/rexfng/restful-auth-api",
  "logo": "https://avatars1.githubusercontent.com/u/10051634",
  "keywords": ["node", "express", "static"],
  "env": {
    "MONGODB_DATABASE_URL": {
            "description": "See https://mlab.com/"
    },
    "EMAIL_PASS": {
            "description": "See: https://sendgrid.com/",
            "required": false
    },
    "IPAPI_API_KEY": {
            "description": "See: https://ipapi.co/#api",
            "required": false
    },
    "CORS": {
            "description": "CORS Domain to whitelist",
            "value": "*",
            "required": false
    },
    "SYSTEM_EMAIL": {
            "description": "<From> email address for reset password email",
            "value": "",
            "required": false
    },
    "APP_NAME": {
            "description": "APP name",
            "value": "APP"
    },
    "TWILIO_API_KEY": {
            "description": "Provide to enable SMS capabilities",
            "required": false
    },
    "TWILIO_SID":{
            "description": "Twilio Requirement",
            "required": false
    },
    "TWILIO_AUTHY_API_KEY":{
            "description": "Provide to enable VOICE AUTH capabilities",
            "required": false
    },
    "AWS_ACCESS_KEY": {
            "description": "For s3upload",
            "required": false
    },
    "AWS_SECRET_KEY": {
            "description": "For s3upload",
            "required": false
    },
    "REDIS_HOST": {
            "description": "For googlesheet db routes caching",
            "required": false
    },
    "REDIS_PORT": {
            "description": "For googlesheet db routes caching",
            "required": false
    } 
  }
}